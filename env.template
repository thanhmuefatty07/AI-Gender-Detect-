# ============================================
# Environment Variables Template
# Copy to .env and fill in your values
# ============================================

# API Keys
YOUTUBE_API_KEY=your_youtube_api_key_here
TIKTOK_SESSION_ID=your_tiktok_session_id_here
INSTAGRAM_SESSION_ID=your_instagram_session_id_here

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/gender_age_db

# Cloud Storage (Optional)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1

GCP_PROJECT_ID=your_gcp_project_id
GCP_CREDENTIALS_PATH=./credentials/gcp-key.json

# Monitoring (Optional)
WANDB_API_KEY=your_wandb_api_key

# API Security
API_SECRET_KEY=your_secret_key_here
JWT_SECRET=your_jwt_secret_here

# Feature Flags
ENABLE_AUTO_LABELING=true
ENABLE_CLOUD_STORAGE=false
ENABLE_MONITORING=true